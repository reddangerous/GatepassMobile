  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "18.0.0",
      "prebuildCommand": "echo 'prebuild'",
      "env": {
        "SKIP_ENV_VALIDATION": "1"
      }
