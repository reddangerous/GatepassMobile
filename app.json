{
  "expo": {
    "name": "GatePass Mobile",
    "slug": "gatepass-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chandaria.png",
    "scheme": "gatepass",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/chandaria.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/chandaria.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "package": "com.gatepass.mobile",
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/chandaria.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/chandaria.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "mdpi": "./assets/images/chandaria.png",
        "hdpi": "./assets/images/chandaria.png",
        "xhdpi": "./assets/images/chandaria.png",
        "xxhdpi": "./assets/images/chandaria.png",
        "xxxhdpi": "./assets/images/chandaria.png"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiUrl": "http://192.168.1.106:3005/api",
      "eas": {
        "projectId": "db238de0-8058-473f-acb0-028623d9a994"
      }
    },
    "owner": "reddangerous",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/db238de0-8058-473f-acb0-028623d9a994"
    }
  }
}
